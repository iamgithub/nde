{
    "app_name":"NDE",
    "frameworks":{
        "frontend":"angular",
        "backend":"mongoose_NOT_USING_YET",
        "html":"hjs_NOT_USING_YET"
    },
    "models":{
        "account":{
            "uri_prefix":"",
            "fields":{
                "email":"email",
                "name": "string",
                "namespace": "namespace"
            }
        },
        "project":{
            "parent":"account",
            "uri_prefix":"",
            "fields":{
                "namespace":"namespace",
                "name":"string",
                "desc":"md",
                "primary_repo":"url"
            }
        },
        "repo":{
            "parent":"project",
            "uri_prefix":"/repos",
            "fields":{
                "namespace":"namespace",
                "name":"string",
                "desc":"md",
                "url":"url"
            }
        },
        "application":{
            "fields":{
                "namespace":"namespace",
                "name":"string",
                "desc":"md",
                "url":"string",
                "owner":{
                    "type":"ref",
                    "ref":"account"
                }
            }
        },
        "accessToken":{
            "parent":"account",
            "uri_prefix":"access_tokens",
            "fields":{
                "perms":{ "type":"array", "sub_type": "String" },
                "token":"string",
                "application":{
                    "type":"ref",
                    "ref":"application"
                },
                "account":{
                    "type":"ref",
                    "ref":"account"
                }
            }
        },
        "requestCode":{
            "fields":{
                "code":"string",
                "application":{
                    "type":"ref",
                    "ref":"application"
                }
            }
        }
    }
}